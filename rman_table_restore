
--RMAN channel configuration for COMMVAULT
configure channel device type sbt_tape PARMS="SBT_LIBRARY=/opt/commvault/Base/libobk.so,BLKSIZE=1048576,ENV=(CvClientName=HOST_NAME,CvInstancename=Instance001)";


run
{
RECOVER TABLE 'SYSADM'.'PSOPRDEFN','SYSADM'.'PSOPRDEFN_LANG','SYSADM'.'PSOPRALIAS','SYSADM'.'PSROLEUSER','SYSADM'.'PSUSERATTR',
'SYSADM'.'PSUSEREMAIL','SYSADM'.'PSUSERPRSNLOPTN','SYSADM'.'PS_ROLEXLATOPR' OF PLUGGABLE DATABASE SISDEV
UNTIL TIME "TO_DATE('19-NOV-2019 08:00', 'DD-MON-YYYY HH24:MI')"
AUXILIARY DESTINATION '/u02/app/oracle/rman' 
REMAP table 'SYSADM'.'PSOPRDEFN':'PSOPRDEFN_bak','SYSADM'.'PSOPRDEFN_LANG':'PSOPRDEFN_LANG_bak','SYSADM'.'PSOPRALIAS':'PSOPRALIAS_bak','SYSADM'.'PSROLEUSER':'PSROLEUSER_bak',
'SYSADM'.'PSUSERATTR':'PSUSERATTR_bak','SYSADM'.'PSUSEREMAIL':'PSUSEREMAIL_bak','SYSADM'.'PSUSERPRSNLOPTN':'PSUSERPRSNLOPTN_bak','SYSADM'.'PS_ROLEXLATOPR':'PS_ROLEXLATOPR_bak';
}


run
{
RECOVER TABLE 'SYSADM'.'PS_RTE_CNTL_RUSER' OF PLUGGABLE DATABASE SISDEV
UNTIL TIME "TO_DATE('19-NOV-2019 08:00', 'DD-MON-YYYY HH24:MI')"
AUXILIARY DESTINATION '/u02/app/oracle/rman' 
REMAP table 'SYSADM'.'PS_RTE_CNTL_RUSER':'PS_RTE_CNTL_RUSER.bak';
}
